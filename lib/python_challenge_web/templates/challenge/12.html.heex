<script type="text/javascript">
async function initialize(pyodide) {
    await pyodide.runPythonAsync(`
    import micropip
    await micropip.install(
        './challenge/wheel/webscrape-1.0.0-py3-none-any.whl'
    )
    from webscrape.chall import *
    `);
    $('#content').html(`
<h1>
    Challenge 12
</h1>
<p>
    There is an array of 100 HTML files in the array "html_files_in_base64", in base64 encoding. In each of these HTML files, there is an input element for "rating". Find and join the ids of these elements to get the solution.
</p>
    `)
}
</script>
